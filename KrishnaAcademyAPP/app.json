{
  "expo": {
    "name": "Krishna Academy",
    "slug": "lms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/App.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/SplashScreen.png",
       "resizeMode": "cover",  
      "backgroundColor": "#c0da7f"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.harsh10021.lms"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "295d8938-c7a6-49ea-872b-12ea8af7db03"
      }
    },
    "owner": "harsh10021"
  }
}
